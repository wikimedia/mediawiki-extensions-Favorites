{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/mediawiki"
	],
	"rules": {
		"jsdoc/check-tag-names": "off",
		"jsdoc/no-undefined-types": "off",
		"jsdoc/require-param-type": "off",
		"jsdoc/require-returns-type": "off",
		"mediawiki/msg-doc": "off",
		"no-prototype-builtins": "off",
		"no-undef": "off",
		"prefer-const": "off"
	}
}
